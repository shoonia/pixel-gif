{
  "name": "pixel-gif",
  "version": "1.0.0",
  "description": "One pixel GIF base64 encoded online generator",
  "type": "module",
  "source": "src/index.html",
  "scripts": {
    "start": "npm run clean && parcel",
    "build": "npm run clean && parcel build",
    "clean": "rm -rf .parcel-cache dist",
    "lint": "eslint 'src/**/*.js' && stylelint 'src/**/*.css'",
    "serve": "python3 -m http.server -d dist"
  },
  "dependencies": {
    "jsx-dom-runtime": "^0.28.0",
    "vanilla-colorful": "^0.7.2"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.2.9",
    "eslint": "^8.34.0",
    "parcel": "^2.8.3",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^30.0.1",
    "typescript": "^4.9.5"
  },
  "targets": {
    "default": {
      "publicUrl": "https://shoonia.github.io/pixel-gif/"
    }
  },
  "author": "Alexander Zaytsev",
  "license": "MIT"
}
